{
  "expo": {
    "name": "ChatMe",
    "slug": "chatme",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "platforms": [
      "ios",
      "android"
    ],
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundModes": [
        "background-processing",
        "background-fetch",
        "background-processing",
        "background-fetch"
      ],
      "bundleIdentifier": "com.chatme1.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs access to your camera for video calls",
        "NSMicrophoneUsageDescription": "This app needs access to your microphone for audio and video calls"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.chatme1.app",
      "allowBackup": false
    },
    "plugins": [
      "expo-router",
      "@daily-co/config-plugin-rn-daily-js",
      [
        "expo-build-properties",
        {
          "android": {
            "minifyEnabled": true,
            "shrinkResources": true,
            "enableProguardInReleaseBuilds": true,
            "enableSeparateBuildPerCPUArchitecture": true,
            "extraProguardRules": "-keep class co.daily.** { *; }\n-keep interface co.daily.** { *; }\n-keep class org.webrtc.** { *; }\n-dontwarn org.webrtc.**"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8ed652b-a93b-4b7e-b07e-2c99e6e1e217"
      }
    }
  }
}
