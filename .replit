args = "npx expo start --host tunnel"

[[workflows.workflow]]
name = "Start Full Stack"
author = 46482010
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd server && npm install && npm start"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --host tunnel"

[[workflows.workflow]]
