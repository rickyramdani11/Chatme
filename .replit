[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/socket-gateway.js"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Gateway"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "API Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Frontend"

[[workflows.workflow]]
name = "Gateway"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/socket-gateway.js"
waitForPort = 8000

[[workflows.workflow]]
name = "API Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/index.js"
waitForPort = 3000

[[workflows.workflow]]
name = "Admin Panel"
author = 47646032
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node server/admin-web.js"

[[workflows.workflow]]
name = "Start Full Stack Fixed"
author = 47646032
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "JWT_SECRET=your_super_secret_key_for_development_only node server/index.js"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "JWT_SECRET=your_super_secret_key_for_development_only node server/socket-gateway.js"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --tunnel --no-web"

[[workflows.workflow]]
name = "Frontend"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx expo start --web --port 8080 --tunnel"
